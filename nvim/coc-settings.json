{
  "languageserver": {
    "kotlin": {
      "command": "~/tools/kotlin-language-server/server/build/install/server/bin/kotlin-language-server",
      "filetypes": ["kotlin"]
    }
  },
  "coc.preferences.formatOnSaveFiletypes": ["markdown", "json"],
  "markdownlint.config": {
    "default": true,
    "line_length": false
  }
}
